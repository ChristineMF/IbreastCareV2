                                <li class="breadcrumb-item active" aria-current="page">DataTables</li>
                            </ol>
                        </div>

                        <!-- Row -->
                        <div class="row">

                            <!-- Sidebar -->

                            <div class="card mb-4">

                                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold text-primary">@Html.ActionLink("Create New", "MydataCreate")</h6>
                                </div>
                                <div class="table-responsive p-3">
                                    <table class="table align-items-center table-flush table-hover" id="dataTableHover">
                                        <thead class="thead-light">
                                            <tr>
                                                @*<th>
                                                @Html.DisplayNameFor(model => model.MyId)
                                            </th>*@
                                                <th>
                                                    @Html.DisplayNameFor(model => model.OperationDate)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Height)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Menopause)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.CellType)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.ER)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.PR)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Her)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.OperationType)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.TreatPlan)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Note)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.InputDate)
                                                </th>
                                                @*<th>
                                                @Html.DisplayNameFor(model => model.UserId)
                                            </th>*@
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    @*<td>
                                                    @Html.DisplayFor(modelItem => item.MyId)
                                                </td>*@
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.OperationDate)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Height)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Menopause)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.CellType)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.ER)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.PR)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Her)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.OperationType)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.TreatPlan)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Note)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.InputDate)
                                                    </td>
                                                    @*<td>
                                                    @Html.DisplayFor(modelItem => item.UserId)
                                                </td>*@
                                                    <td>

                                                        @Html.ActionLink("Edit", "Edit", new { id = item.MyId }) |
                                                        @Html.ActionLink("Details", "Details", new { id = item.MyId }) |
                                                        @Html.ActionLink("Delete", "Delete", new { id = item.MyId })
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
